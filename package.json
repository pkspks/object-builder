{
  "name": "ts-object-builder",
  "version": "0.0.7",
  "description": "Simple Typesafe Object builder for Typescript",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "lint": "tslint 'src/**/*.ts'",
    "test": "npm run lint && npm run test-only",
    "test-only": "env TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' mocha -r ts-node/register --reporter mocha-multi --reporter-options spec=-,xunit=./test-results/test-results.xml src/*.spec.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pkspks/ts-object-builder.git"
  },
  "keywords": [
    "typescript",
    "spec",
    "test",
    "factory",
    "angular"
  ],
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/mocha": "^5.0.0",
    "chai": "^4.1.2",
    "mocha": "^5.1.0",
    "mocha-multi": "^1.0.1",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "typescript": "^2.4"
  },
  "author": "pkspks",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pkspks/ts-object-builder/issues"
  },
  "homepage": "https://github.com/pkspks/ts-object-builder#readme",
  "dependencies": {}
}
